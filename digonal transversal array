#include <iostream>
using namespace std;

int main()
{
    
    int arr[4][4]={ {11,12,13,14},
    {21,22,23,24},
    {31,32,33,34},
    {41,42,43,44}};
    for(int i=0;i<4;i++){
        int idx=0;
        for(int j=0+i;j<4;j++){
            cout<<arr[idx][j]<<" ";
            idx++;
        }
    }

    return 0;
}
